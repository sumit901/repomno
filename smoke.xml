<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test thread-count="5" name="LoginTest">
    <classes>
      <class name="groupingtests.Login">
      <methods>
      <include name="loginWithValidCredentials"></include>
      <include name="loginWithInvalidCredentials"></include>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
   <test thread-count="5" name="RegisterTest">
    <classes>
      <class name="groupingtests.Register">
      <methods>
      <include name="registerByProvidingOnlyMandatoryFields"></include>
      <include name="registerByProvidingAllFields"></include>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
   <test thread-count="5" name="SearchTest">
    <classes>
      <class name="groupingtests.Search">
      <methods>
      <include name="searchForValidProduct"></include>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->

