<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<groups>
<run>
<include name="windows.*"></include>
</run>
</groups>
  <test thread-count="5" name="RegisterTest">
    <classes>
      <class name="groupingtests.Register"/>
