<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test thread-count="5" name="Testone">
    <classes>
      <class name="com.tutorialsninja.com.annotations.Login">
      <methods>
      <include name="verifyUserLoginWithValidCredentials"></include>
      <include name="verifyUserLoginWithValidUsernameInvalidPassword"></include>
      <include name="verifyUserLoginWithInvalidUsernameValidPassword"></include>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Testtwo">
    <classes>
      <class name="com.tutorialsninja.com.annotations.Login">
      <methods>
      <include name="verifyUserLoginWithInvalidUsernameInvalidPassword"></include>
      <include name="verifyUserLoginWithoutGivingCredentials"></include>
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
